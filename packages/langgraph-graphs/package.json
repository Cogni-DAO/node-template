{
  "name": "@cogni/langgraph-graphs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.js"
    },
    "./graphs": {
      "types": "./dist/graphs/index.d.ts",
      "import": "./dist/graphs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "node -e \"const fs=require('fs'); if(!fs.existsSync('dist/index.js')){console.error('ERROR: dist/index.js missing'); process.exit(1)}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:ci": "vitest run --config vitest.config.ts"
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.7",
    "@cogni/ai-core": "workspace:*",
    "@cogni/ai-tools": "workspace:*",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}
