{
  "name": "@cogni/scheduler-worker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "node -e \"const fs=require('fs'); if(!fs.existsSync('dist/index.js')){console.error('ERROR: dist/index.js missing'); process.exit(1)}\"",
    "typecheck": "tsc --noEmit",
    "dev": "tsx watch src/worker.ts",
    "start": "tsx src/worker.ts"
  },
  "dependencies": {
    "graphile-worker": "^0.16.6",
    "cron-parser": "^4.9.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "tsx": "^4.19.0",
    "typescript": "^5"
  }
}
