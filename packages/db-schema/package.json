{
  "name": "@cogni/db-schema",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./refs": {
      "types": "./dist/refs.d.ts",
      "import": "./dist/refs.js"
    },
    "./scheduling": {
      "types": "./dist/scheduling.d.ts",
      "import": "./dist/scheduling.js"
    },
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js"
    },
    "./billing": {
      "types": "./dist/billing.d.ts",
      "import": "./dist/billing.js"
    },
    "./ai": {
      "types": "./dist/ai.d.ts",
      "import": "./dist/ai.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "node -e \"const fs=require('fs'); for(const f of ['dist/index.js','dist/refs.js']){if(!fs.existsSync(f)){console.error('ERROR: '+f+' missing'); process.exit(1)}}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.7"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}
