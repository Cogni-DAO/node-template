{
  "name": "@cogni/scheduler-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "node -e \"const fs=require('fs'); if(!fs.existsSync('dist/index.js')){console.error('ERROR: dist/index.js missing'); process.exit(1)}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cogni/ai-core": "workspace:*",
    "@cogni/db-schema": "workspace:*",
    "@cogni/ids": "workspace:*",
    "type-fest": "^5.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}
