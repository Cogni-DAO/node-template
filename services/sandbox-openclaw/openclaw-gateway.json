  "agents": {
    "defaults": {
      "model": { "primary": "cogni/claude-opus-4.6" },
      "subagents": {
        "model": "cogni/gemini-3-flash",
        "maxConcurrent": 3,
        "archiveAfterMinutes": 30
      },
      "sandbox": { "mode": "off" },
      "skipBootstrap": true,
      "timeoutSeconds": 540,
      "heartbeat": { "every": "0" },
      "memorySearch": {
        "enabled": true,
        "provider": "openai",
        "model": "text-embedding-3-small",
        "remote": {
          "baseUrl": "http://llm-proxy-openclaw:8080/v1",
          "apiKey": "proxy-handles-auth",
          "batch": { "enabled": false }
        },
        "extraPaths": ["/repo/current/docs", "/repo/current/work"]
      }
    },
    "list": [
      { "id": "main", "default": true, "workspace": "/workspace/gateway" }
    ]
  },
  "skills": {
    "load": {
      "extraDirs": ["/repo/current/.openclaw/skills"]
    }
  },
  "tools": {
    "elevated": { "enabled": false },
    "deny": ["browser", "cron", "gateway", "nodes", "sessions_send", "message"]
  },
  "cron": { "enabled": false },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": { "mode": "token", "token": "${OPENCLAW_GATEWAY_TOKEN}" },
    "http": { "endpoints": { "chatCompletions": { "enabled": true } } }
  }
}
