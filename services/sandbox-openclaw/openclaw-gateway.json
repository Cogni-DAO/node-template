  "agents": {
    "defaults": {
      "model": { "primary": "cogni/deepseek-v3.2" },
      "thinkingDefault": "high",
      "subagents": {
        "model": "cogni/gpt-4o-mini",
        "maxConcurrent": 3,
        "archiveAfterMinutes": 30
      },
      "verboseDefault": "off",
      "sandbox": { "mode": "off" },
      "skipBootstrap": true,
      "timeoutSeconds": 540,
      "heartbeat": { "every": "0" },
      "models": {
        "cogni/gpt-4o-mini": {},
        "cogni/gpt-5.2": {},
        "cogni/gpt-5": {},
        "cogni/claude-sonnet-4.5": { "params": { "cacheRetention": "long" } },
        "cogni/claude-opus-4.5": { "params": { "cacheRetention": "long" } },
        "cogni/deepseek-v3.1": {},
        "cogni/deepseek-v3.2": {},
        "cogni/gemini-2.5-flash": {},
        "cogni/gemini-3-pro": {},
        "cogni/gemini-3-flash": {},
        "cogni/kimi-k2-thinking": {},
        "cogni/kimi-k2.5": {},
        "cogni/minimax-m2": {},
        "cogni/qwen3-vl-8b": {},
        "cogni/grok-4.1-fast": {},
        "cogni/llama-3.3-70b": {}
      },
      "contextPruning": { "mode": "cache-ttl", "ttl": "1h" },
      "memorySearch": {
        "enabled": false,
        "provider": "openai",
        "model": "text-embedding-3-small",
        "remote": {
          "baseUrl": "http://llm-proxy-openclaw:8080/v1",
          "apiKey": "proxy-handles-auth",
          "batch": { "enabled": false }
        },
        "extraPaths": ["/repo/current/docs", "/repo/current/work"]
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "workspace": "/workspace/gateway",
        "subagents": {
          "allowAgents": ["*"]
        }
      }
    ]
  },
  "skills": {
    "load": {
      "extraDirs": ["/repo/current/.openclaw/skills"]
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "token": "${DISCORD_BOT_TOKEN}",
      "groupPolicy": "open",
      "guilds": {
        "*": {
          "requireMention": true,
          "channels": {
            "*": {
              "autoThread": true
            }
          }
        }
      }
    }
  },
  "plugins": {
    "entries": {
      "discord": { "enabled": true }
    }
  },
  "tools": {
    "elevated": { "enabled": false },
    "deny": [
      "browser",
      "cron",
      "gateway",
      "message",
      "nodes",
      "sessions_list",
      "sessions_history"
    ]
  },
  "cron": { "enabled": false },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": { "mode": "token", "token": "${OPENCLAW_GATEWAY_TOKEN}" },
    "http": { "endpoints": { "chatCompletions": { "enabled": true } } }
  }
}
