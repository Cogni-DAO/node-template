{
  "models": {
    "mode": "replace",
    "providers": {
      "cogni": {
        "baseUrl": "http://llm-proxy-openclaw:8080/v1",
        "api": "openai-completions",
        "apiKey": "proxy-handles-auth",
        "models": [
          {
            "id": "test-model",
            "name": "Test Model",
            "reasoning": false,
            "input": ["text"],
            "contextWindow": 200000,
            "maxTokens": 8192,
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 }
          },
          {
            "id": "test-free-model",
            "name": "Free Test Model",
            "reasoning": false,
            "input": ["text"],
            "contextWindow": 200000,
            "maxTokens": 8192,
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 }
          },
          {
            "id": "test-paid-model",
            "name": "Paid Test Model",
            "reasoning": false,
            "input": ["text"],
            "contextWindow": 200000,
            "maxTokens": 8192,
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 }
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": { "primary": "cogni/test-model" },
      "sandbox": { "mode": "off" },
      "skipBootstrap": true,
      "timeoutSeconds": 540,
      "heartbeat": { "every": "0" }
    },
    "list": [
      { "id": "main", "default": true, "workspace": "/workspace/current" }
    ]
  },
  "tools": {
    "elevated": { "enabled": false },
    "deny": [
      "browser",
      "cron",
      "gateway",
      "nodes",
      "sessions_send",
      "sessions_spawn",
      "message"
    ]
  },
  "cron": { "enabled": false },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": { "mode": "token", "token": "${OPENCLAW_GATEWAY_TOKEN}" },
    "http": { "endpoints": { "chatCompletions": { "enabled": true } } }
  }
}
